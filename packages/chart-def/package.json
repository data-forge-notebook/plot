  "bugs": {
    "url": "https://github.com/data-forge-notebook/plot/issues"
  },
  "homepage": "https://github.com/data-forge-notebook/plot#readme",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.12.3",
    "@microsoft/api-extractor": "^7.12.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@plotex/serialization": "^1.0.11"
  }
