{
  "name": "@plotex/render-image",
  "version": "1.0.8",
  "description": "A plug in for plot: The simplest plotting API for JavaScript and TypeScript.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "setup": "npm install --save=false plot",
    "c": "npm run clean",
    "clean": "rm -rf build/* && rm -rf template/",
    "b": "npm run build",
    "build": "tsc",
    "build:template": "cd template-src && webpack",
    "cb": "npm run clean-build",
    "clean-build": "npm run clean && npm run build",
    "build:watch": "tsc --watch",
    "clean-build:watch": "rm -rf build/* && tsc --watch",
    "prepublishOnly": "npm run setup &&  npm test && npm run clean && npm run build:template && npm run build",
    "t": "npm run test",
    "test": "jest",
    "tw": "npm run test:watch",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/data-forge-notebook/plot.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "plot",
    "data",
    "visualization",
    "visualisation",
    "analysis"
  ],
  "author": "ashley@codecapers.com.au",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/data-forge-notebook/plot/issues"
  },
  "homepage": "https://github.com/data-forge-notebook/plot#readme",
  "dependencies": {
    "capture-template": "^1.1.18",
    "opn": "^6.0.0"
  },
  "peerDependencies": {
    "plot": "^1.0.8"
  },
  "devDependencies": {
    "@plotex/lib-apex": "^1.0.8",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "copy-webpack-plugin": "^5.0.2",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
}
